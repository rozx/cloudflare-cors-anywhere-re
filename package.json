{
    "name": "cloudflare-cors-anywhere",
    "version": "1.3.1",
    "description": "A CORS proxy running as a Cloudflare Worker",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "update-version": "node scripts/update-version.js",
        "predeploy": "npm run update-version",
        "deploy": "wrangler deploy --var VERSION:$(node -p \"require('./package.json').version\")",
        "logs": "wrangler tail",
        "logs:json": "wrangler tail --format json"
    },
    "keywords": [
        "cors",
        "proxy",
        "cloudflare",
        "worker",
        "cloudflare-workers"
    ],
    "author": "rozx",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/rozx/cloudflare-cors-anywhere.git"
    },
    "bugs": {
        "url": "https://github.com/rozx/cloudflare-cors-anywhere/issues"
    },
    "homepage": "https://github.com/rozx/cloudflare-cors-anywhere#readme",
    "devDependencies": {
        "husky": "^2.1.0",
        "prettier": "^1.17.0",
        "wrangler": "^4.51.0"
    }
}
